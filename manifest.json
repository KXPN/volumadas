{
  "name": "Volumadas",
  "description": "",
  "version": "2.0.1",
  "manifest_version": 3,
  "permissions": ["storage"],
  "options_page": "configuracion.html",
  "content_scripts": [
    {
      "matches": ["https://meet.google.com/*"],
      "js": ["volumadas.js"]
    },
    {
      "matches": ["https://hangouts.google.com/*/call/*"],
      "js": ["volumadas.js"]
    }
  ],
  "icons": {
    "16": "icono_16.png",
    "48": "icono_48.png",
    "128": "icono_128.png"
  }
}
